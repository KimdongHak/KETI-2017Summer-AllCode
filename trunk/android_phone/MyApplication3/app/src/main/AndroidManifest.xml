<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.c.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/c"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity_continue"
            android:label="@string/title_activity_continue"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity_detail"
            android:label="광주 지하철"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".activity_send"
            android:label="광주 지하철"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateVisible|adjustPan|stateAlwaysHidden"></activity>
        <activity android:name=".activity_survey"></activity>
    </application>

</manifest>